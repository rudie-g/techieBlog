{{#if loggedIn}}
{{#each postData as |post|}}
    <div class="post-container">
        <h4>{{title}}</h4>
        <p>{{content}}</p>
        <p>{{date}}</p>
    </div>
{{/each}}
<div class="d-flex justify-content-center">
<a id="post-button" class="link" href="/new">Got Something To Say?</a>
</div>
{{/if}}